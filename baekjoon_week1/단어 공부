#include <iostream>

using namespace std;

int main() {
	string s;
	cin >> s;

	int arr[26] = {0, }; // 알파벳별 빈도수
	int cnt = 0; // 최대 빈도수 개수
	int max = 0; // 최대 빈도수
	int result_index = 0;
	
	for (int i = 0; i < s.length(); i++) { // 한글자씩 count하기
		int index = 0;
		if (s[i] < 97) index = s[i] - 65; // A면 65-65=0
		else index = s[i] - 97; // a면 97-97=0
		arr[index]++;
	}

	for (int i = 0; i < 26; i++) { // 최대빈도수 구하기
		if (max < arr[i]) {
			max = arr[i];
			result_index = i;
		}
	}

	for (int i = 0; i < 26; i++) { // 최대빈도수 개수
		if (arr[i] == max) cnt++;
	}

	if (cnt >= 2) cout << "?";
	else cout << (char)(result_index+65);

}
